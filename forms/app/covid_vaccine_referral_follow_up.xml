      <bind nodeset="/covid_vaccine_referral_follow_up/referral_details/covid_vaccine_type" type="select1" relevant="selected(../vaccine_dose, 'yes')" required="true()"/>
      <bind nodeset="/covid_vaccine_referral_follow_up/referral_details/appointment_date_known" type="select1" relevant="selected(../dose_incompletion_reason, 'upcoming_appointment')" required="true()"/>
      <bind nodeset="/covid_vaccine_referral_follow_up/referral_details/next_visit" type="date" constraint="decimal-date-time(.) &gt;= decimal-date-time(today())" jr:constraintMsg="jr:itext('/covid_vaccine_referral_follow_up/referral_details/next_visit:jr:constraintMsg')" relevant=" /covid_vaccine_referral_follow_up/referral_details/appointment_date_known  = 'yes'" required="true()"/>
      <bind nodeset="/covid_vaccine_referral_follow_up/referral_details/no_visit_reason" type="select1" relevant=" /covid_vaccine_referral_follow_up/referral_details/visited_facility  = 'no'"/>
      <bind nodeset="/covid_vaccine_referral_follow_up/referral_details/n_encourage_completion1" readonly="true()" type="string" relevant="selected(../no_visit_reason, 'severe_side_effects')" required="true()"/>
      <bind nodeset="/covid_vaccine_referral_follow_up/referral_details/n_encourage_completion2" readonly="true()" type="string" relevant="selected(../no_visit_reason, 'vaccine_not_effective')" required="true()"/>
      <bind nodeset="/covid_vaccine_referral_follow_up/referral_details/n_encourage_completion3" readonly="true()" type="string" relevant="selected(../no_visit_reason, 'health_facility_far') or selected(../no_visit_reason, 'no_transport') or selected(../no_visit_reason, 'long_queue')" required="true()"/>
      <bind nodeset="/covid_vaccine_referral_follow_up/referral_details/no_visit_other" type="string" relevant="selected(../no_visit_reason, 'other')" required="true()"/>
      <bind nodeset="/covid_vaccine_referral_follow_up/referral_details/n_encourage_vaccination" readonly="true()" type="string" relevant=" /covid_vaccine_referral_follow_up/referral_details/visited_facility  = 'no'" required="true()"/>
      <bind nodeset="/covid_vaccine_referral_follow_up/group_review/r_report" readonly="true()" type="string"/>
      <bind nodeset="/covid_vaccine_referral_follow_up/group_review/submit" readonly="true()" type="string"/>
      <bind nodeset="/covid_vaccine_referral_follow_up/group_review/r_summary_details" readonly="true()" type="string"/>
      <bind nodeset="/covid_vaccine_referral_follow_up/group_review/r_patient_details" readonly="true()" type="string"/>
      <bind nodeset="/covid_vaccine_referral_follow_up/group_review/r_findings" readonly="true()" type="string"/>
      <bind nodeset="/covid_vaccine_referral_follow_up/group_review/r_finding_visited" readonly="true()" type="string" relevant=" /covid_vaccine_referral_follow_up/referral_details/visited_facility  = 'yes'"/>
      <bind nodeset="/covid_vaccine_referral_follow_up/group_review/r_finding_not_visited" readonly="true()" type="string" relevant=" /covid_vaccine_referral_follow_up/referral_details/visited_facility  = 'no'"/>
      <bind nodeset="/covid_vaccine_referral_follow_up/meta/instanceID" type="string" readonly="true()" calculate="concat('uuid:', uuid())"/>
