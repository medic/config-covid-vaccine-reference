'yes', 'no')"/>
      <bind nodeset="/covid_vaccine_referral_follow_up/t_death_confirmation_date" type="string" calculate="today()"/>
      <bind nodeset="/covid_vaccine_referral_follow_up/vaccination_details/patient_available" type="select1" required="true()"/>
      <bind nodeset="/covid_vaccine_referral_follow_up/vaccination_details/patient_availability_follow_up" type="date" constraint="decimal-date-time(.) &gt;= decimal-date-time(today()) and decimal-date-time(.) &lt;= decimal-date-time(today()) + 7" jr:constraintMsg="jr:itext('/covid_vaccine_referral_follow_up/vaccination_details/patient_availability_follow_up:jr:constraintMsg')" relevant="selected(../patient_available, 'no')" required="true()"/>
      <bind nodeset="/covid_vaccine_referral_follow_up/referral_details" relevant=" /covid_vaccine_referral_follow_up/vaccination_details/patient_available  = 'yes'"/>
      <bind nodeset="/covid_vaccine_referral_follow_up/referral_details/visited_facility" type="select1" relevant=" /covid_vaccine_referral_follow_up/vaccination_details/patient_available  = 'yes'" required="true()"/>
      <bind nodeset="/covid_vaccine_referral_follow_up/referral_details/vaccine_dose" type="select1" relevant=" /covid_vaccine_referral_follow_up/referral_details/visited_facility  = 'yes'" required="true()"/>
