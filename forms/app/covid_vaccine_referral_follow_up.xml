      <bind nodeset="/covid_vaccine_referral_follow_up/patient_age_in_weeks" type="string" calculate="floor((decimal-date-time(today()) - decimal-date-time( /covid_vaccine_referral_follow_up/inputs/contact/date_of_birth )) div 7)"/>
      <bind nodeset="/covid_vaccine_referral_follow_up/patient_gender" type="string" calculate="../inputs/contact/sex"/>
