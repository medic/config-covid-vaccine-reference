      <bind nodeset="/covid_vaccine_follow_up/patient_age_in_days" type="string" calculate="floor( decimal-date-time(today()) - decimal-date-time( /covid_vaccine_follow_up/inputs/contact/date_of_birth ) )"/>
      <bind nodeset="/covid_vaccine_follow_up/patient_age_in_weeks" type="string" calculate="floor((decimal-date-time(today()) - decimal-date-time( /covid_vaccine_follow_up/inputs/contact/date_of_birth )) div 7)"/>
      <bind nodeset="/covid_vaccine_follow_up/patient_gender" type="string" calculate="../inputs/contact/sex"/>
      <bind nodeset="/covid_vaccine_follow_up/t_death_confirmation" type="string" calculate="if(selected(../referral_completion/condition, 'dead'), 
'yes', 'no')"/>
      <bind nodeset="/covid_vaccine_follow_up/t_death_confirmation_date" type="string" calculate="today()"/>
      <bind nodeset="/covid_vaccine_follow_up/vaccination_details/patient_available" type="select1" required="true()"/>
