      <bind nodeset="/covid_vaccine_follow_up/follow_up_details/next_facility_visit" type="date" jr:constraintMsg="jr:itext('/covid_vaccine_follow_up/follow_up_details/next_facility_visit:jr:constraintMsg')" relevant=" /covid_vaccine_follow_up/follow_up_details/vaccine_dose  = 'no'" constraint="decimal-date-time(.) &gt;= decimal-date-time(today()) and decimal-date-time(.) &lt;= decimal-date-time(today()) + 14" required="true()"/>
      <bind nodeset="/covid_vaccine_follow_up/follow_up_details/no_visit_reason" type="select1" relevant=" /covid_vaccine_follow_up/follow_up_details/visited_facility  = 'no'"/>
      <bind nodeset="/covid_vaccine_follow_up/follow_up_details/n_encourage_completion1" readonly="true()" type="string" relevant="selected(../no_visit_reason, 'severe_side_effects')" required="true()"/>
      <bind nodeset="/covid_vaccine_follow_up/follow_up_details/n_encourage_completion2" readonly="true()" type="string" relevant="selected(../no_visit_reason, 'vaccine_not_effective')" required="true()"/>
