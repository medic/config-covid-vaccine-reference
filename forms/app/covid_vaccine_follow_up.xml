      <bind nodeset="/covid_vaccine_follow_up/patient_id" type="string" required="true()" calculate="../inputs/contact/_id"/>
      <bind nodeset="/covid_vaccine_follow_up/patient_name" type="string" calculate="../inputs/contact/name"/>
      <bind nodeset="/covid_vaccine_follow_up/patient_date_of_birth" type="string" calculate="../inputs/contact/date_of_birth"/>
      <bind nodeset="/covid_vaccine_follow_up/patient_age_in_years" type="string" calculate="floor( difference-in-months(  /covid_vaccine_follow_up/inputs/contact/date_of_birth , today() ) div 12 )"/>
