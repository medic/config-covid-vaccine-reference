      <bind nodeset="/covid_vaccine_follow_up/follow_up_details" relevant=" /covid_vaccine_follow_up/vaccination_details/patient_available  = 'yes'"/>
      <bind nodeset="/covid_vaccine_follow_up/follow_up_details/visited_facility" type="select1" relevant=" /covid_vaccine_follow_up/vaccination_details/patient_available  = 'yes'" required="true()"/>
